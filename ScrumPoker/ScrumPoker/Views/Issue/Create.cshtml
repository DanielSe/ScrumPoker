@{
    ViewBag.Title = "Create";
}

@model ScrumPoker.Models.Issue

<div class="container">
    <h1>Create a new Issue for room @Model.Room.Name</h1>
    
    @using (Html.BeginForm())
    {
        @Html.EditorForModel()

        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    }
</div>
